{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "es5",
      "es6"
    ],    
    "outDir": "./dist",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "strict": true,         
    "strictNullChecks": false,   
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}